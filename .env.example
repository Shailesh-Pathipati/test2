# Free LLM API Keys (Get these from respective providers)

# Groq (Fast inference, free tier available)
# Get API key from: https://console.groq.com/
EXPO_PUBLIC_GROQ_API_KEY=your_groq_api_key_here

# Together AI (Free tier available)
# Get API key from: https://api.together.xyz/
EXPO_PUBLIC_TOGETHER_API_KEY=your_together_api_key_here

# Cohere (Free tier available)
# Get API key from: https://dashboard.cohere.ai/
EXPO_PUBLIC_COHERE_API_KEY=your_cohere_api_key_here

# Hugging Face (Free inference API)
# Get API key from: https://huggingface.co/settings/tokens
EXPO_PUBLIC_HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Optional: AWS Configuration (for future AWS integration)
EXPO_PUBLIC_AWS_REGION=us-east-1
EXPO_PUBLIC_AWS_ACCESS_KEY_ID=your_aws_access_key
EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Development settings
EXPO_PUBLIC_MOCK_AI_RESPONSES=false
EXPO_PUBLIC_DEBUG_MODE=true